    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>": "List of file copy entries.",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.copyName.<VALUES>": "Logical name used for logging/display.",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.src.<VALUES>": "Source file path.",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.dest.<VALUES>": "Destination file path.",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>": "List of zip archive entries.",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.zipName.<VALUES>": "Name of the zip archive.",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.src.<VALUES>": "Folder to zip.",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.output.<VALUES>": "Output zip file path.",
    "<MODEL_ID>.Backup.<VALUES>.CheckPath.<VALUES>": "Path that must exist for the backup job to be considered valid."
