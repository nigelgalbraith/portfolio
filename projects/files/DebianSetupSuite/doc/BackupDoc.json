  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<MODEL_ID>.Backup.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFolders.<VALUES>.copyName.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFolders.<VALUES>.src.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFolders.<VALUES>.dest.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.copyName.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.src.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CopyFiles.<VALUES>.dest.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.zipName.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.src.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.ZipArchives.<VALUES>.output.<VALUES>",
    "<MODEL_ID>.Backup.<VALUES>.CheckPath.<VALUES>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Backup": {
        "DebianSuite": {
          "CopyFolders": [
            {
              "copyName": "DebianConstants",
              "src": "$HOME/Documents/GitHub/DebianScripts/constants",
              "dest": "$HOME/Documents/GitHub/Backups/Debian/constants"
            }
          ],
          "CopyFiles": [
            {
              "copyName": "DebianLoader",
              "src": "$HOME/Documents/GitHub/DebianScripts/DebianLoader.py",
              "dest": "$HOME/Documents/GitHub/Backups/Debian/DebianLoader.py"
            }
          ],
          "ZipArchives": [
            {
              "zipName": "DebianScriptsSetupTools",
              "src": "$HOME/Documents/GitHub/Backups/Debian",
              "output": "$HOME/Documents/GitHub/Backups/DebianScriptsSetupTools.zip"
            }
          ],
          "CheckPath": "$HOME/Documents/GitHub/Backups/Debian"
        },
        "TextCreator": {
          "CopyFolders": [
            {
              "copyName": "TextCreator",
              "src": "$HOME/Documents/GitHub/AI/TextCreator",
              "dest": "$HOME/Documents/GitHub/Backups/TextCreator"
            }
          ],
          "CopyFiles": [],
          "ZipArchives": [
            {
              "zipName": "TextCreator",
              "src": "$HOME/Documents/GitHub/Backups/TextCreator",
              "output": "$HOME/Documents/GitHub/Backups/TextCreator.zip"
            }
          ],
          "CheckPath": "$HOME/Documents/GitHub/Backups/TextCreator"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>.Backup.<VALUES>": "A backup job definition.",
