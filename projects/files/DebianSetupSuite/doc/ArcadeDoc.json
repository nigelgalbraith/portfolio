  "EXAMPLE": {
    "<MODEL_ID>": {
      "Arcade": {
        "mame": {
          "Packages": [
            "mame",
            "mame-data",
            "mame-tools"
          ],
          "SetupDirs": [
            "/etc/mame",
            "/var/lib/arcade/mame"
          ],
          "SetupScripts": [
            {
              "script": "settings/controllers/install_xbox_controller.sh",
              "args": []
            }
          ],
          "SetupCmds": [
            "/usr/games/mame -listxml > /var/lib/arcade/mame.xml"
          ],
          "ChmodPaths": [
            {
              "path": "/var/lib/arcade/mame",
              "mode": "2775",
              "recursive": true
            }
          ],
          "ChownUser": "nigel",
          "ChownGroup": "arcade",
          "ChownRecursive": true,
          "ChownPaths": [
            {
              "path": "/var/lib/arcade/mame"
            }
          ],
          "RemovePaths": [
            "/etc/mame/*.ini"
          ],
          "ResetDirs": [
            "/etc/mame"
          ],
          "ResetCmds": [
            "bash -lc 'cd /etc/mame && /usr/games/mame -createconfig'"
          ],
          "SettingsFiles": [
            {
              "copyName": "MainConfig",
              "src": "settings/mame/Desktop/Desktop-mame_template.ini",
              "dest": "/etc/mame/mame.ini"
            }
          ],
          "SettingsFolders": [],
          "arcadeUsers": [
            "nigel"
          ],
          "arcadeGroups": [
            "arcade"
          ],
          "ProtectedFolders": [
            {
              "path": "/mnt/plexmedia/PlexMedia/arcade",
              "owner": "root",
              "group": "arcade",
              "permissions": "3775"
            }
          ]
        },
        "retroarch": {
          "Packages": [
            "retroarch",
            "libretro-core-info"
          ],
          "SetupDirs": [
            "/etc/retroarch",
            "/var/lib/arcade/retroarch"
          ],
          "SetupScripts": [],
          "SetupCmds": [],
          "ChmodPaths": [
            {
              "path": "/var/lib/arcade/retroarch",
              "mode": "2775",
              "recursive": true
            }
          ],
          "ChownUser": "nigel",
          "ChownGroup": "arcade",
          "ChownRecursive": true,
          "ChownPaths": [
            {
              "path": "/var/lib/arcade/retroarch"
            }
          ],
          "RemovePaths": [
            "/etc/retroarch/retroarch.cfg"
          ],
          "ResetDirs": [
            "/etc/retroarch"
          ],
          "ResetCmds": [],
          "SettingsFiles": [
            {
              "copyName": "RetroArchConfig",
              "src": "settings/retroarch/Desktop/Desktop-template-retroarch.cfg",
              "dest": "/etc/retroarch/retroarch.cfg"
            }
          ],
          "SettingsFolders": [],
          "arcadeUsers": [
            "nigel"
          ],
          "arcadeGroups": [
            "arcade"
          ],
          "ProtectedFolders": [
            {
              "path": "/mnt/plexmedia/PlexMedia/arcade",
              "owner": "root",
              "group": "arcade",
              "permissions": "3775"
            }
          ]
        }
      }
    }
  },
