{
  "FIXED_FIELDS": [
    "Networks",
    "ConnectionName",
    "Interface",
    "Address",
    "Gateway",
    "DNS"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<NETWORK_PROFILE>",
    "<values for the keys above>"
