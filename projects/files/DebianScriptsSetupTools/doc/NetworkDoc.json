  "EXAMPLE": {
    "<MODEL_ID>": {
      "Networks": {
        "OfficeLAN": {
          "ConnectionName": "value",
          "Interface": "value",
          "Address": "value",
          "Gateway": "value",
          "DNS": "value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "Networks": {
        "<NETWORK_PROFILE>": {
          "ConnectionName": "Describe what 'ConnectionName' is used for.",
          "Interface": "Describe what 'Interface' is used for.",
          "Address": "Describe what 'Address' is used for.",
          "Gateway": "Describe what 'Gateway' is used for.",
          "DNS": "Describe what 'DNS' is used for."
        }
      }
    }
  }
}
