{
  "FIXED_FIELDS": [
    "Archive",
    "DownloadURL",
    "ExtractTo",
    "StripTopLevel",
    "DownloadPath"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<APP_NAME>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Archive": {
        "example_app": {
          "DownloadURL": "https://example.com/file",
          "ExtractTo": "/path/to/value",
          "StripTopLevel": false,
          "DownloadPath": "/path/to/value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "Archive": {
        "<APP_NAME>": {
          "DownloadURL": "Describe what 'DownloadURL' is used for.",
          "ExtractTo": "Describe what 'ExtractTo' is used for.",
          "StripTopLevel": "Describe what 'StripTopLevel' is used for.",
          "DownloadPath": "Describe what 'DownloadPath' is used for."
        }
      }
    }
  }
}
