    "DOSLoader",
    "Name",
    "DownloadURL",
    "ExtractTo",
    "CheckPath",
    "StripTopLevel",
    "LaunchCmd"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<GAME_ID>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "DOSLoader": {
        "example_game": {
          "Name": "value",
          "DownloadURL": "https://example.com/file",
          "ExtractTo": "/path/to/value",
          "CheckPath": "/path/to/value",
          "StripTopLevel": false,
          "LaunchCmd": "value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "DOSLoader": {
        "<GAME_ID>": {
          "Name": "Describe what 'Name' is used for.",
          "DownloadURL": "Describe what 'DownloadURL' is used for.",
          "ExtractTo": "Describe what 'ExtractTo' is used for.",
          "CheckPath": "Describe what 'CheckPath' is used for.",
          "StripTopLevel": "Describe what 'StripTopLevel' is used for.",
          "LaunchCmd": "Describe what 'LaunchCmd' is used for."
        }
      }
    }
  }
