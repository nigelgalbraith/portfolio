{
  "FIXED_FIELDS": [
    "Packages"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<PACKAGE_NAME>",
    "<add/remove package keys>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Packages": {
        "vlc": {},
        "audacity": {},
        "firefox-esr": {}
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "Packages": {
        "<PACKAGE_NAME>": "Add a package name as a key. In the real config, the value stays {}."
      }
    }
  }
}
