{
  "FIXED_FIELDS": [
    "Docker",
    "Name",
    "Image",
    "Location"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<CONTAINER_NAME>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Docker": {
        "example_container": {
          "Name": "value",
          "Image": "value",
          "Location": "/path/to/value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "Docker": {
        "<CONTAINER_NAME>": {
          "Name": "Describe what 'Name' is used for.",
          "Image": "Describe what 'Image' is used for.",
          "Location": "Describe what 'Location' is used for."
        }
      }
    }
  }
}
