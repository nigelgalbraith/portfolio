{
  "FIXED_FIELDS": [
    "ThirdParty",
    "repo_name",
    "url",
    "key",
    "codename",
    "component",
    "keyring_dir"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<PACKAGE_NAME>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "ThirdParty": {
        "example-package": {
          "repo_name": "value",
          "url": "https://example.com/file",
          "key": "value",
          "codename": "value",
          "component": "value",
          "keyring_dir": "/path/to/value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "ThirdParty": {
        "<PACKAGE_NAME>": {
          "repo_name": "Describe what 'repo_name' is used for.",
          "url": "Describe what 'url' is used for.",
          "key": "Describe what 'key' is used for.",
          "codename": "Describe what 'codename' is used for.",
          "component": "Describe what 'component' is used for.",
          "keyring_dir": "Describe what 'keyring_dir' is used for."
        }
      }
    }
  }
}
