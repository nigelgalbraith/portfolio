{
  "FIXED_FIELDS": [
    "Flatpak",
    "remote"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<FLATPAK_APP_ID>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Flatpak": {
        "org.videolan.VLC": {
          "remote": "flathub"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "Flatpak": {
        "<FLATPAK_APP_ID>": {
          "remote": "Describe what 'remote' is used for."
        }
      }
    }
  }
}
