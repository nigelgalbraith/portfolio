  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<MODEL_ID>.Firewall.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.Applications.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.RuleName.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.Port.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.Protocol.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.IPs.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.RuleName.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.From.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.To.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.Protocol.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.IPs.<VALUES>",
    "<MODEL_ID>.Firewall.<VALUES>.ICMP.Allow.<VALUES>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "Firewall": {
        "Home Network": {
          "Applications": [
            "KTorrent"
          ],
          "SinglePorts": [
            {
              "RuleName": "SSH",
              "Port": 22,
              "Protocol": "tcp",
              "IPs": [
                "192.168.4.50",
                "192.168.10.50"
              ]
            },
            {
              "RuleName": "Plex",
              "Port": 32400,
              "Protocol": "tcp",
              "IPs": [
                "192.168.4.100",
                "192.168.10.100"
              ]
            }
          ],
          "PortRanges": [],
          "ICMP": {
            "Allow": true
          }
        },
        "Office Network": {
          "Applications": [
            "OpenSSH"
          ],
          "SinglePorts": [
            {
              "RuleName": "RDP",
              "Port": 3389,
              "Protocol": "tcp",
              "IPs": [
                "10.0.0.10",
                "10.0.0.20"
              ]
            }
          ],
          "PortRanges": [
            {
              "RuleName": "Ephemeral UDP",
              "From": 32768,
              "To": 60999,
              "Protocol": "udp",
              "IPs": [
                "10.0.0.0/24"
              ]
            }
          ],
          "ICMP": {
            "Allow": false
          }
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>.Firewall.<VALUES>": "A firewall rule group (e.g. Home Network, Office Network).",
    "<MODEL_ID>.Firewall.<VALUES>.Applications.<VALUES>": "List of predefined firewall application profiles to allow (e.g. OpenSSH, KTorrent).",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.RuleName.<VALUES>": "Human-readable name for the firewall rule.",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.Port.<VALUES>": "Single TCP/UDP port number to match.",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.Protocol.<VALUES>": "Network protocol: tcp or udp.",
    "<MODEL_ID>.Firewall.<VALUES>.SinglePorts.<VALUES>.IPs.<VALUES>": "List of source IPs or CIDR ranges allowed to access this port.",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.RuleName.<VALUES>": "Human-readable name for the port range rule.",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.From.<VALUES>": "Start of the port range (inclusive).",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.To.<VALUES>": "End of the port range (inclusive).",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.Protocol.<VALUES>": "Network protocol: tcp or udp.",
    "<MODEL_ID>.Firewall.<VALUES>.PortRanges.<VALUES>.IPs.<VALUES>": "List of source IPs or CIDR ranges allowed for this port range.",
    "<MODEL_ID>.Firewall.<VALUES>.ICMP.<VALUES>.Allow.<VALUES>": "If true, ICMP (ping) traffic is allowed; if false, it is blocked."
  }
