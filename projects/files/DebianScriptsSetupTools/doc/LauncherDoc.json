  "EXAMPLE": {
    "<MODEL_ID>": {
      "Launcher": {
        "flex-launcher": {
          "DownloadURL": "https://github.com/complexlogic/flex-launcher/releases/download/v2.2/flex-launcher_2.2_amd64.deb",
          "download_dir": "/tmp/debian_loader_downloads",
          "AdditionalPackages": [
            "unclutter",
            "xinput",
            "xdotool"
          ],
          "DefaultDM": {
            "name": "gdm3",
            "config_file": "/etc/gdm3/custom.conf"
          },
          "KioskDM": {
            "name": "lightdm",
            "config_file": "/etc/lightdm/lightdm.conf"
          },
          "SettingsFiles": [
            {
              "copyName": "Flex launcher config",
              "src": "settings/launcher/flex-launcher.ini",
              "dest": "/etc/flex-launcher/flex-launcher.ini"
            }
          ],
          "SettingsFolders": [
            {
              "copyName": "Launcher assets",
              "src": "settings/launcher/assets",
              "dest": "/opt/flex-launcher/assets"
            }
          ],
          "SettingsChmodPaths": [
            {
              "path": "/opt/flex-launcher",
              "mode": "0755"
            }
          ],
          "SettingsChownPaths": [
            {
              "path": "/opt/flex-launcher",
              "user": "root",
              "group": "root"
            }
          ],
          "RemovePaths": [
            "/opt/old-launcher",
            "/etc/flex-launcher.old"
          ],
          "SetupDirs": [
            "/opt/flex-launcher",
            "/var/lib/flex-launcher"
          ],
          "KioskFiles": [
            "/etc/xdg/autostart/flex-launcher.desktop"
          ],
          "ChmodPaths": [
            {
              "path": "/opt/flex-launcher/flex-launcher",
              "mode": "0755"
            }
          ],
          "ChownPaths": [
            {
              "path": "/var/lib/flex-launcher"
            }
          ],
          "ChownUser": "launcher",
          "ChownRecursive": true
        },
        "simple-kiosk": {
          "DownloadURL": "https://example.com/releases/simple-kiosk_1.0_amd64.deb",
          "download_dir": "/tmp/debian_loader_downloads",
          "AdditionalPackages": [
            "unclutter",
            "xrandr"
          ],
          "DefaultDM": {
            "name": "gdm3",
            "config_file": "/etc/gdm3/custom.conf"
          },
          "KioskDM": {
            "name": "lightdm",
            "config_file": "/etc/lightdm/lightdm.conf"
          },
          "SettingsFiles": [
            {
              "copyName": "Kiosk config",
              "src": "settings/launcher/simple-kiosk.conf",
              "dest": "/etc/simple-kiosk/simple-kiosk.conf"
            }
          ],
          "SettingsFolders": [],
          "SettingsChmodPaths": [],
          "SettingsChownPaths": [],
          "RemovePaths": [
            "/etc/simple-kiosk.old"
          ],
          "SetupDirs": [
            "/opt/simple-kiosk",
            "/var/lib/simple-kiosk"
          ],
          "KioskFiles": [
            "/etc/xdg/autostart/simple-kiosk.desktop"
          ],
          "ChmodPaths": [
            {
              "path": "/opt/simple-kiosk/simple-kiosk",
              "mode": "0755"
            }
          ],
          "ChownPaths": [
            {
              "path": "/var/lib/simple-kiosk"
            }
          ],
          "ChownUser": "kiosk",
          "ChownRecursive": true
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>.Launcher.<VALUES>": "A launcher/kiosk install and configuration job.",
