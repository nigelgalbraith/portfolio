{
  "FIXED_FIELDS": [
    "DEB",
    "DownloadURL",
    "EnableService",
    "download_dir"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<PACKAGE_NAME>",
    "<values for the keys above>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "DEB": {
        "example-package": {
          "DownloadURL": "https://example.com/file",
          "EnableService": false,
          "download_dir": "/path/to/value"
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>": {
      "DEB": {
        "<PACKAGE_NAME>": {
          "DownloadURL": "Describe what 'DownloadURL' is used for.",
          "EnableService": "Describe what 'EnableService' is used for.",
          "download_dir": "Describe what 'download_dir' is used for."
        }
      }
    }
  }
}
