{
  "FIXED_FIELDS": [
    "<MODEL_ID>.StartupServices",
    "<MODEL_ID>.StartupServices.<VALUES>.Order",
    "<MODEL_ID>.StartupServices.<VALUES>.Name",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptSrc",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptDest",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceSrc",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceDest",
    "<MODEL_ID>.StartupServices.<VALUES>.LogPath",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigSrc",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigDest"
  ],
  "EDITABLE_FIELDS": [
    "<MODEL_ID>",
    "<MODEL_ID>.StartupServices.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.Order.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.Name.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptSrc.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptDest.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceSrc.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceDest.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.LogPath.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigSrc.<VALUES>",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigDest.<VALUES>"
  ],
  "EXAMPLE": {
    "<MODEL_ID>": {
      "StartupServices": {
        "my-startup-task": {
          "Order": 10,
          "Name": "my-startup-task",
          "ScriptSrc": "settings/services/my-startup-task.sh",
          "ScriptDest": "/usr/local/bin/my-startup-task.sh",
          "ServiceSrc": "settings/services/my-startup-task.service",
          "ServiceDest": "/etc/systemd/system/my-startup-task.service",
          "LogPath": "/var/log/my-startup-task.log",
          "ConfigSrc": "settings/services/my-startup-task.conf",
          "ConfigDest": "/etc/my-startup-task.conf"
        },
        "network-warmup": {
          "Order": 20,
          "Name": "network-warmup",
          "ScriptSrc": "settings/services/network-warmup.sh",
          "ScriptDest": "/usr/local/bin/network-warmup.sh",
          "ServiceSrc": "settings/services/network-warmup.service",
          "ServiceDest": "/etc/systemd/system/network-warmup.service",
          "LogPath": "/var/log/network-warmup.log",
          "ConfigSrc": "",
          "ConfigDest": ""
        }
      }
    }
  },
  "DESCRIPTION": {
    "<MODEL_ID>.StartupServices.<VALUES>": "A startup service job executed during system boot/startup.",
    "<MODEL_ID>.StartupServices.<VALUES>.Order.<VALUES>": "Execution ordering number (lower runs first). Useful when multiple services depend on each other.",
    "<MODEL_ID>.StartupServices.<VALUES>.Name.<VALUES>": "Service name identifier (used for filenames, systemd unit name, etc).",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptSrc.<VALUES>": "Source script path in your repo/project.",
    "<MODEL_ID>.StartupServices.<VALUES>.ScriptDest.<VALUES>": "Destination path on the system where the script will be installed.",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceSrc.<VALUES>": "Source systemd unit file in your repo/project.",
    "<MODEL_ID>.StartupServices.<VALUES>.ServiceDest.<VALUES>": "Destination systemd unit path (commonly /etc/systemd/system/<name>.service).",
    "<MODEL_ID>.StartupServices.<VALUES>.LogPath.<VALUES>": "Where the service/script should write logs (if your script uses it).",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigSrc.<VALUES>": "Optional source config file shipped with the project (blank if none).",
    "<MODEL_ID>.StartupServices.<VALUES>.ConfigDest.<VALUES>": "Optional destination path for the config file (blank if none)."
  }
}
