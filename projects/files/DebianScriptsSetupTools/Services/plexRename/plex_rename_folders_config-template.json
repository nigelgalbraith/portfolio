{
  "plex_folder_locs": {
    "/mnt/plexmedia/PlexMedia/Movies": {
      "include_files": true,
      "expand_subfolders": true,
      "expand_exceptions": ["Subs", "Subtitles", "Extras", "Artwork"],
      "suffixes": ["(\\([12][0-9]{3}\\)|[\\s\\-][12][0-9]{3})"],
      "suffix_exceptions": ["Blade Runner 2049 (2017)"],
      "char_replacements": {
        "^(www\\.UIndex\\.org[\\s]*-[\\s]*)": "",
        "\\.": " ",
        " 20([0-9]{2})$": " (20\\1)",
        " 19([0-9]{2})$": " (19\\1)"
      },
      "replace_order": [
        "^(www\\.UIndex\\.org[\\s]*-[\\s]*)",
        "\\.",
        " 20([0-9]{2})$",
        " 19([0-9]{2})$"
      ],
      "replace_exceptions": ["A.I. Artificial Intelligence (2001)"],
      "remove_empty_folders": true,
      "remove_empty_exceptions": ["Subs", "Subtitles", "Extras", "Artwork"],
      "trash_dir": "/root/.local/share/Trash/files",
      "folder_organization": {
        ".srt": "Subs"
      }
    },
    "/mnt/plexmedia/PlexMedia/TV Series": {
      "include_files": true,
      "expand_subfolders": true,
      "expand_exceptions": ["Subs", "Subtitles", "Extras", "Artwork", "re:(?i)Season \\d+"],
      "suffixes": [
        "(\\([12][0-9]{3}\\)|[\\s\\-][12][0-9]{3})",
        "S[0-9]{2}E[0-9]{2}"
      ],
      "suffix_exceptions": [],
      "char_replacements": {
        "\\.": " ",
        "(?i)\\bs(\\d{2})e(\\d{2})\\b": "S\\1E\\2",
        " 20([0-9]{2})$": " (20\\1)",
        " 19([0-9]{2})$": " (19\\1)"
      },
      "replace_order": [
        "\\.",
        "(?i)\\bs(\\d{2})e(\\d{2})\\b",
        " 20([0-9]{2})$",
        " 19([0-9]{2})$"
      ],
      "replace_exceptions": [],
      "remove_empty_folders": true,
      "remove_empty_exceptions": ["Subs", "Subtitles", "Extras", "Artwork", "re:(?i)Season \\d+"],
      "trash_dir": "/root/.local/share/Trash/files",
      "folder_organization": {
        ".srt": "Subs",
        "S##E##": "Season #"
      }
    }
  }
}
