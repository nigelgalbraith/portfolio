{
  "Default": {
    "Docker": {
      "webrcade": {
        "Name": "webrcade",
        "Image": "webrcade/webrcade",
        "Port": "8080:80",
        "Location": "~/projects/webArcade/webrcade-master/",
        "Download": "https://codeload.github.com/webrcade/webrcade/zip/refs/heads/master",
        "TmpDir": "/tmp/docker_downloads"
      },
      "dataanalysis-tool": {
        "Name": "dataanalysis-tool",
        "Location": "~/projects/dataAnalysis/dataanalysis-tool/",
        "Download": "https://raw.githubusercontent.com/nigelgalbraith/portfolio.github.io/main/projects/files/DataAnalysisTool.zip",
        "TmpDir": "/tmp/docker_downloads",
        "ComposeFile": "~/projects/dataAnalysis/dataanalysis-tool/docker-compose.yml",
        "ComposeDir": "~/projects/dataAnalysis/dataanalysis-tool",
        "ComposeContainers": [
          "dataanalysis-postgres",
          "dataanalysis-api",
          "dataanalysis-web"
        ],
        "Tests": [
          {"label": "heathcheck", "path": "~/Documents/GitHub/DebianScripts/tests/common/healthcheck.py"},
          {"label": "api-heathcheck", "path": "~/Documents/GitHub/DebianScripts/tests/dataanalysis-tool/routes/api_health.py"}
        ]
      },
      "language-translator": {
        "Name": "language-translator",
        "Location": "~/projects/AI/LanguageTranslator/",
        "Download": "https://raw.githubusercontent.com/nigelgalbraith/portfolio.github.io/main/projects/files/LanguageTranslator.zip",
        "TmpDir": "/tmp/docker_downloads",
        "ComposeFile": "~/projects/AI/LanguageTranslator/docker-compose.yml",
        "ComposeDir": "~/projects/AI/LanguageTranslator",
        "ComposeContainers": [
          "translate-ui",
          "piper-en",
          "piper-es",
          "piper-zh"
        ],
        "Tests": [
          {"label": "heathcheck", "path": "~/Documents/GitHub/DebianScripts/tests/common/healthcheck.py"}
        ]
      },
      "text-creator": {
        "Name": "text-creator",
        "Location": "~/projects/AI/TextCreator/",
        "Download": "https://raw.githubusercontent.com/nigelgalbraith/portfolio.github.io/main/projects/files/TextCreator.zip",
        "TmpDir": "/tmp/docker_downloads",
        "ComposeFile": "~/projects/AI/TextCreator/docker-compose.yml",
        "ComposeDir": "~/projects/AI/TextCreator",
        "ComposeContainers": [
          "text-generator-ui",
          "ollama",
          "piper"
        ],
        "Tests": [
          {"label": "heathcheck", "path": "~/Documents/GitHub/DebianScripts/tests/common/healthcheck.py"}
        ]
      }
    }
  }
}
